{
    "extends": "tslint:latest",
    "rules": {
        "curly": true,
        "one-variable-per-declaration": [
            true
        ],
        "no-any": true,
        "no-consecutive-blank-lines": [
            true
        ],
        "no-require-imports": true,
        "member-access": [
            true
        ],
        "indent": [
            true,
            "spaces"
        ],
        "object-literal-sort-keys": false,
        "object-literal-shorthand": false,
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration"
        ],
        "max-line-length": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "no-unused-variable": [
            true
        ]
    }
}